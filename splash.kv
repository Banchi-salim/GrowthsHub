#:set s dp(160)
Splash:
    name:"splash"
    MDFloatLayout:
        md_bg_color:37/255, 170/255, 225/255, 1
        Image:
            source:"images/growthshubAsset3.png"
            pos_hint:{"center_x":.5, "center_y":.55}
            size_hint: .26, .26
            canvas.before:
                Color:
                    rgb:1, 1, 1, 1
                Ellipse:
                    pos:self.center_x - (s/2), self.center_y - (s/2),
                    size: s, s
                    #width:3



        MDLabel:
            text:"GrowthsHub"
            pos_hint:{"center_x":.68, "center_y":.3}
            font_size:"32dp"
